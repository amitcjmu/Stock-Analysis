      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  model_name AS operation,\n  llm_provider AS service,\n  COUNT(*) AS call_count,\n  AVG(response_time_ms) AS avg_response_ms,\n  PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY response_time_ms) AS p95_response_ms,\n  MAX(response_time_ms) AS max_response_ms,\n  SUM(CASE WHEN NOT success THEN 1 ELSE 0 END) AS error_count\nFROM migration.llm_usage_logs\nWHERE $__timeFilter(created_at)\nGROUP BY model_name, llm_provider\nORDER BY call_count DESC;",
          "refId": "A"
        }
      ],
      "title": "LLM Call Latency by Model (from llm_usage_logs)",
      "description": "Response time metrics from LLM usage logs - complements tracing data",
      "type": "table"
    }
