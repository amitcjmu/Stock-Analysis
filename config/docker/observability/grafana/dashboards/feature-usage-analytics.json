          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  ca.name AS client_name,\n  e.name AS engagement_name,\n  COUNT(*) AS call_count,\n  SUM(l.total_cost) AS total_cost,\n  SUM(l.total_tokens) AS total_tokens,\n  COUNT(DISTINCT l.flow_id) AS unique_flows\nFROM migration.llm_usage_logs l\nLEFT JOIN migration.client_accounts ca ON l.client_account_id = ca.id\nLEFT JOIN migration.engagements e ON l.engagement_id = e.id\nWHERE $__timeFilter(l.created_at)\nGROUP BY ca.name, e.name\nORDER BY call_count DESC\nLIMIT 20;",
          "refId": "A"
        }
