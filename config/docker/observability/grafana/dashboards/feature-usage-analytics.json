        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  flow_type,\n  COUNT(*) AS total_flows,\n  SUM(CASE WHEN flow_status = 'completed' THEN 1 ELSE 0 END) AS completed,\n  SUM(CASE WHEN flow_status = 'running' THEN 1 ELSE 0 END) AS running,\n  SUM(CASE WHEN flow_status = 'failed' THEN 1 ELSE 0 END) AS failed,\n  ROUND(\n    SUM(CASE WHEN flow_status = 'completed' THEN 1 ELSE 0 END)::numeric /\n    NULLIF(COUNT(*), 0)::numeric * 100,\n    1\n  ) AS completion_rate,\n  ROUND(\n    AVG(CASE WHEN flow_status = 'completed' THEN\n      EXTRACT(EPOCH FROM (updated_at - created_at)) / 60\n    END)::numeric,\n    1\n  ) AS avg_duration_min\nFROM migration.crewai_flow_state_extensions\nWHERE $__timeFilter(created_at)\nGROUP BY flow_type\nORDER BY total_flows DESC;",
          "refId": "A"
        }
      ],
