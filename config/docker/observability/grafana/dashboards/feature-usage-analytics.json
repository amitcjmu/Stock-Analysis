          "rawSql": "SELECT\n  'discovery' AS flow_type,\n  current_phase AS phase,\n  COUNT(*) AS flow_count\nFROM migration.discovery_flows\nWHERE $__timeFilter(created_at)\nGROUP BY current_phase\nUNION ALL\nSELECT\n  'assessment',\n  current_phase,\n  COUNT(*)\nFROM migration.assessment_flows\nWHERE $__timeFilter(created_at)\nGROUP BY current_phase\nUNION ALL\nSELECT\n  'collection',\n  current_phase::text,\n  COUNT(*)\nFROM migration.collection_flows\nWHERE $__timeFilter(created_at)\nGROUP BY current_phase\nORDER BY flow_type, flow_count DESC;",
