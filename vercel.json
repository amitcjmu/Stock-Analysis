{
  "env": {
    "VITE_BACKEND_URL": "https://migrate-ui-orchestrator-production.up.railway.app"
  },
  "build": {
    "env": {
      "VITE_BACKEND_URL": "https://migrate-ui-orchestrator-production.up.railway.app"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
